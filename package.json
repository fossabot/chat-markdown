{
    "name": "chat-markdown",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "build": "pnpm run i18n --no-watch && vite build",
        "i18n": "typesafe-i18n",
        "check": "tsx ./scripts/check.ts",
        "clean": "rm -rf dist node_modules pnpm-lock.yaml",
        "dev": "vite",
        "format": "rome format . --write && pnpm check",
